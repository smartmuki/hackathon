<script>
	function insertMixerDiv() { 
	var sibling = document.getElementsByClassName("pi-product-metadata tertiary");

var overlayMainDiv = document.createElement("div");
overlayMainDiv.style.gridColumnStart = "2";
overlayMainDiv.style.gridColumnEnd = "3";
overlayMainDiv.style.gridRowStart = "auto";
overlayMainDiv.style.msGridColumn = "2";
overlayMainDiv.style.msGridColumnSpan = "1";
overlayMainDiv.style.zIndex = "100";
overlayMainDiv.style.paddingLeft = "166px";


var overlayContainer = document.createElement("div");
overlayContainer.style.width = "100%";
overlayContainer.style.height = "50px";
overlayContainer.style.backgroundColor = "#29374a";

overlayMainDiv.appendChild(overlayContainer);


var divContainer = document.createElement("div");
divContainer.setAttribute("id", "mixerFrame");
//divContainer.style.padding = "12px 0 0 0";
//divContainer.style.paddingRight = "96px";
divContainer.style.gridColumnStart = "2";
divContainer.style.gridColumnEnd = "3";
divContainer.style.gridRowStart = "auto";
divContainer.style.msGridColumn = "2";
divContainer.style.msGridColumnSpan = "1";
divContainer.style.msGridRow = "4";
divContainer.style.position = "relative";
divContainer.style.zIndex = "1";
divContainer.style.marginBottom = "24px";
divContainer.style.marginTop = "-50px";

sibling[0].parentNode.insertBefore(divContainer, sibling[0].nextSibling);
divContainer = document.getElementById("mixerFrame");
divContainer.parentNode.insertBefore(overlayMainDiv, divContainer);



var iframe = document.createElement("iframe");
iframe.setAttribute("src", "https://mixer.com/QueenEliminator");
iframe.setAttribute("id", "mixeriFrame");
iframe.width = "100%";
iframe.height = "650px";
iframe.style.border = "0";

divContainer.appendChild(iframe);
}
	setTimeout(function() { 
		var interval = setInterval(function() { 
			var sibling = document.getElementsByClassName("pi-product-metadata tertiary");
			if (sibling && sibling.length) { 
				clearInterval(interval);
				insertMixerDiv();
			} 
		}, 2000)
	}, 1000);
</script>
